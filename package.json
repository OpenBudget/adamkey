{
  "name": "adamkey",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "copy-license": "cp LICENSE dist/adamkey",
    "copy-readme": "cp README.md dist/adamkey",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "build_lib": "ng build adamkey",
    "npm_pack": "cd dist/adamkey && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run npm_pack",
    "publish": "npm run package && npm publish ./dist/adamkey/adamkey*.tgz"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "~17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@types/d3-array": "^3.0.6",
    "@types/d3-path": "^3.0.0",
    "@types/d3-scale": "^4.0.4",
    "@types/d3-selection": "^3.0.6",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^17.1.2",
    "typescript": "~5.3.3"
  }
}