{
  "name": "adamkey",
  "version": "0.0.3",
  "description": "A component for displaying a horizontal bar graph of many items in one series",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "start": "webpack-dev-server",
    "build-app": "webpack",
    "watch": "tsc -p src -w",
    "build:styles": "lessc --clean-css --source-map ./assets/styles/adamkey.less ./assets/styles/adamkey.css",
    "build:scripts": "tsc -p src",
    "prebuild": "rm -rf lib",
    "build": "npm run build:scripts && npm run build:styles",
    "prepublish": "npm run build",
    "install-into": "./install-into.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenBudget/adamkey.git"
  },
  "keywords": [
    "BarChart",
    "Angular 2",
    "OpenBudget"
  ],
  "author": "adam.kariv@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenBudget/adamkey/issues"
  },
  "homepage": "https://github.com/OpenBudget/adamkey#readme",
  "devDependencies": {
    "awesome-typescript-loader": "^3.2.3",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "less-plugin-clean-css": "^1.5.1",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.0",
    "typescript": "^2.4.2",
    "uglify-js-plugin": "0.0.6",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.1.2",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "@angular/animations": "^5.2.9",
    "@angular/common": "^5.2.9",
    "@angular/compiler": "^5.2.9",
    "@angular/compiler-cli": "^5.2.9",
    "@angular/core": "^5.2.9",
    "@angular/forms": "^5.2.9",
    "@angular/http": "^5.2.9",
    "@angular/platform-browser": "^5.2.9",
    "@angular/platform-browser-dynamic": "^5.2.9",
    "@angular/platform-server": "^5.2.9",
    "@angular/router": "^5.2.9",
    "@types/node": "^8.10.11",
    "core-js": "^2.5.1",
    "reflect-metadata": "^0.1.10",
    "rxjs": "^5.5.8",
    "zone.js": "^0.6.26"
  }
}
